{
  "name": "hackernews",
  "version": "0.1.11",
  "private": true,
  "author": "laffery",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Laffery/alipreview/issues"
  },
  "homepage": "https://github.com/Laffery/alipreview#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laffery/alipreview.git"
  },
  "scripts": {
    "dev": "webpack serve --config config/webpack.dev.js",
    "start": "webpack serve --port 3000  --config config/webpack.prod.js",
    "build": "rm -rf dist/ && webpack build --config config/webpack.base.js",
    "lint": "npx eslint src --color",
    "test": "echo \"NO TEST MODULE FOUND\"",
    "pretty": "prettier --write"
  },
  "lint-staged": {
    "hackernews/**/*.{js,jsx,ts,tsx,md,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "hackernews/*.css": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@typescript-eslint/typescript-estree": "5.15.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.0",
    "react-dev-utils": "^12.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.0",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
