{
  "name": "hackernews",
  "version": "0.2.0",
  "private": true,
  "author": "laffery",
  "bugs": {
    "url": "https://github.com/Laffery/alipreview/issues"
  },
  "homepage": "https://github.com/Laffery/alipreview#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laffery/alipreview.git"
  },
  "types": "src/typings/*",
  "scripts": {
    "dev": "export NODE_ENV=development && concurrently -k -p name -n \"client,server,server\" -c \"yellow,green,blue\" \"yarn dev:client\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\"",
    "start": "export NODE_ENV=production && nodemon dist/server/index.js",
    "build": "export NODE_ENV=production && yarn build:client && yarn build:server",
    "clean:build": "rm -rf dist",
    "clean": "rm -rf dist && rm -rf node_modules",
    "test": "echo NO TEST MODULE FOUND",
    "dev:client": "webpack -c webpack/webpack.dev.js -w",
    "dev:server:webpack": "webpack -c webpack/webpack.server.js -w",
    "dev:server:nodemon": "wait-on -l dist/server/index.js && yarn start",
    "build:client": "rm -rf dist/client && webpack build -c webpack/webpack.prod.js",
    "build:server": "rm -rf dist/server && webpack build -c webpack/webpack.server.js"
  },
  "lint-staged": {
    "hackernews/**/*.{js,jsx,ts,tsx,md,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "hackernews/*.css": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "concurrently": "^7.0.0",
    "express": "^4.17.3",
    "fs-extra": "^10.0.1",
    "node-fetch": "~2.6.7",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serialize-javascript": "^6.0.0",
    "url-parse": "^1.5.10",
    "wait-on": "^6.0.1",
    "@types/query-string": "^6.3.0",
    "ahooks": "^3.1.13",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react-router-dom": "^6.2.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "~2.6.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/serialize-javascript": "^5.0.2",
    "@types/url-parse": "^1.4.8",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0",
    "@types/lodash": "^4.14.180",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@typescript-eslint/typescript-estree": "5.15.0",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "prettier": "^2.6.0",
    "react-dev-utils": "^12.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.0"
  },
  "license": "MIT"
}
